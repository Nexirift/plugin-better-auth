{
	"name": "@nexirift/plugin-better-auth",
	"version": "1.0.3",
	"type": "commonjs",
	"description": "An experimental plugin for GraphQL Yoga that adds Better Auth functionality.",
	"author": "Nexirift",
	"license": "MIT",
	"engines": {
		"node": ">=18.0.0"
	},
	"scripts": {
		"check": "tsc --pretty --noEmit",
		"build": "unbuild",
		"prepack": "unbuild"
	},
	"peerDependencies": {
		"better-auth": ">=1.3.4",
		"graphql-yoga": ">=5.15.1",
		"ioredis": ">=5.6.1",
		"redis": ">=4.7.1"
	},
	"peerDependenciesMeta": {
		"ioredis": {
			"optional": true
		},
		"redis": {
			"optional": true
		}
	},
	"devDependencies": {
		"@types/node": "^24.1.0",
		"graphql": "^16.11.0",
		"graphql-scalars": "^1.24.2",
		"typescript": "^5.8.3",
		"unbuild": "^3.6.0"
	},
	"publishConfig": {
		"directory": "dist",
		"access": "public"
	},
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		}
	},
	"main": "./dist/index.cjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist/**",
		"dev/**"
	]
}
